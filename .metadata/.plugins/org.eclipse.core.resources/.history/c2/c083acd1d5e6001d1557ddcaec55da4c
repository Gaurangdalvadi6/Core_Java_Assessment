package core_assessment;


//Class
public class Product {
	// Instance variables
	private  int id,price,quantity;
	private String name,company;
	
	// Default Constructor
	public Product() {}

	public Product(int id, int price, int quantity, String name, String company) {
		
		this.id = id;
		this.price = price;
		this.quantity = quantity;
		this.name = name;
		this.company = company;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}

	public int getQuantity() {
		return quantity;
	}

	public void setQuantity(int quantity) {
		this.quantity = quantity;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getCompany() {
		return company;
	}

	public void setCompany(String company) {
		this.company = company;
	}

	// toString() Method
    // @return
	@Override
	public String toString() {
		return "Product [id=" + id + ", price=" + price + ", quantity=" + quantity + ", name=" + name + ", company="
				+ company + "]";
	}
	
	
	
	
}
